# Пример файла конфигурации для UniQuest
# Скопируйте этот файл в .env и заполните своими данными

# Безопасность
SECRET_KEY=your-secret-key-here-change-this-in-production
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1,*

# База данных PostgreSQL
DB_NAME=uniquestus
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_HOST=localhost
DB_PORT=5432

# Для production измените:
# DEBUG=False
# ALLOWED_HOSTS=yourdomain.com,www.yourdomain.com,your-ip-address

